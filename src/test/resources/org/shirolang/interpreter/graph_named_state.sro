node Box begin
     input length Double
     input width Double
     input height Double
     update Multiply(length, width)
     output area Double(update)
     output name String("Box")
end


graph box_calc begin
    b Box
    b.length(100.0)
    b.width(20.0)
    b.height(7.0)
end


state s1 begin
    graph box_calc
end
