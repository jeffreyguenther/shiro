node Box[vol] begin
    input length Double
    input width Double
    input height Double
    option base Multiply(length, width)
    option vol Multiply(length, width * height)
    output volume Double(vol)
    output area Double(base)
    output name String("Box")
end

b Box[base](length: 100.0, width: 20.0, height: 7.0)
